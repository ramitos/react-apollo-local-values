{
  "name": "react-apollo-local-values",
  "version": "1.0.1",
  "license": "BSD-3-Clause",
  "description": "easily get and set values from a apollo local state",
  "keywords": ["apollo", "react", "apollo-link-state", "set", "get"],
  "repository": "github:ramitos/react-apollo-local-values",
  "main": "dist/react-apollo-local-values.umd.js",
  "jsnext:main": "dist/react-apollo-local-values.es.js",
  "module": "dist/react-apollo-local-values.es.js",
  "entry": "src/index.js",
  "files": ["dist"],
  "scripts": {
    "lint": "eslint . --fix --ext .js --ext .md",
    "fmt": "prettier --config .prettierrc --write '**/*'",
    "build": "NODE_ENV=development bup",
    "prepublish": "NODE_ENV=production bup",
    "watch": "onchange 'app/**/*.js' 'src/*.js' -- npm run build"
  },
  "dependencies": {
    "camel-case": "^3.0.0",
    "force-array": "^3.1.0",
